{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Hyperjump JSON Client",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "/Applications/Visual Studio Code.app/Contents/MacOS/Electron",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/client",
        "--user-data-dir=${workspaceFolder}/.vscode-dev-userdata",
        "--extensions-dir=${workspaceFolder}/.vscode-dev-extensions",
        "--disable-extension=vscode.json-language-features",
        "--disable-extension=vscode.typescript-language-features",
        "${workspaceFolder}"
      ],
      "outFiles": ["${workspaceFolder}/client/dist/**/*.js"],
      "preLaunchTask": "Build All"
    }
  ]
}
